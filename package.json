{
  "name": "upp",
  "description": "Uptime monitor and status page powered by GitHub Actions, Issues, and Pages",
  "version": "0.0.0",
  "author": "Nirmitjatana @Nirmitjatana",
  "bin": {
    "upp": "./bin/run"
  },
  "bugs": "https://github.com/upptime/cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "cli-ux": "^5.6.3",
    "fs-extra": "^10.0.0",
    "js-yaml": "^4.1.0",
    "node-cron": "^3.0.0",
    "node-libcurl": "^2.3.3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.12",
    "@types/js-yaml": "^4.0.2",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/node-cron": "^2.0.4",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "rimraf": "^3.0.2",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/upptime/cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "upp",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "upptime/cli",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
